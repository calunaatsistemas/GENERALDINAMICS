                    <property
                      dataType="System.Int32"
                      description="The number of seconds before a command times out.  A value of 0 indicates an infinite time-out."
                      name="CommandTimeout">0</property>
                    <property
                      dataType="System.String"
                      description="Specifies the name of the database object used to open a rowset."
                      name="OpenRowset"></property>
                    <property
                      dataType="System.String"
                      description="Specifies the variable that contains the name of the database object used to open a rowset."
                      name="OpenRowsetVariable"></property>
                    <property
                      dataType="System.String"
                      description="The SQL command to be executed."
                      name="SqlCommand"
                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=15.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">SELECT  -- FecProceso,
		ID_Calendario,
		-- CodCentro,
		ID_Centro,
		-- Cod_Sexo,
		ID_Sexo,
		(SELECT COUNT(*) FROM Dimension.Dim_Empleado DMNA
				WHERE DMNA.Fec_Alta &lt;= FecProceso
					AND DMNA.[Cod_Centro] = CodCentro
					AND DMNA.[Cod_Sexo] = Cod_Sexo) AS Num_Altas,
		(SELECT COUNT(*) FROM Dimension.Dim_Empleado DMNB
				WHERE DMNB.Fec_Baja &lt;= FecProceso
					AND DMNB.[Cod_Centro] = CodCentro
					AND DMNB.[Cod_Sexo] = Cod_Sexo) AS Num_Bajas
	FROM (
	SELECT DISTINCT Dimension.Dim_Empleado.Fec_Alta AS FecProceso, 
			Dimension.Calendario.idCalendario AS ID_Calendario, 
			Dimension.Dim_Centro.CodCentro AS CodCentro,
			Dimension.Dim_Centro.Id_Centro AS ID_Centro,
			Dimension.Dim_Sexo.Cod_Sexo AS Cod_Sexo, 
			Dimension.Dim_Sexo.Id_Sexo AS ID_Sexo
	FROM   Dimension.Dim_Empleado INNER JOIN
				 Dimension.Calendario ON Dimension.Dim_Empleado.Fec_Alta = Dimension.Calendario.Fecha INNER JOIN
				 Dimension.Dim_Sexo ON Dimension.Dim_Empleado.Cod_Sexo = Dimension.Dim_Sexo.Cod_Sexo INNER JOIN
				 Dimension.Dim_Centro ON Dimension.Dim_Empleado.Cod_Centro = Dimension.Dim_Centro.CodCentro, Dimension.Dim_Centro dc
	WHERE (Dimension.Dim_Empleado.Ind_Activo = 1)) X  ORDER BY X.ID_Calendario DESC</property>
